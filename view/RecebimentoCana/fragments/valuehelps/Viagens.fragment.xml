<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<TableSelectDialog noDataText="Nenhuma viagem encontrada" title="Selecionar Viagem" search="handleValueHelpSearchViagens"
		liveChange="handleValueHelpSearchViagens" confirm="handleCloseViagens" multiSelect="false" growingThreshold="500" items="{Cargas}">
		<ColumnListItem>
			<cells>
				<Text text="{idViagem}"/>
				<Text text="{descricaoViagem}"/>
			</cells>
		</ColumnListItem>
		<columns>
			<Column width="12em">
				<header></header>
			</Column>
			<Column width="12em">
				<header></header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>