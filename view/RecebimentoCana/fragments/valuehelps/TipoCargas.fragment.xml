<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<TableSelectDialog noDataText="Nenhuma tipo de carga foi encontrado" title="Selecionar Tipo de Cargas" search="handleValueHelpSearchTipoCargas"
		liveChange="handleValueHelpSearchTipoCargas" confirm="handleCloseTipoCargas" multiSelect="false" growingThreshold="500" items="{TipoCargas}">
		<ColumnListItem>
			<cells>
				<Text text="{idTipoCarga}"/>
				<Text text="{descricaoTipoCarga}"/>
			</cells>
		</ColumnListItem>
		<columns>
			<Column width="12em">
				<header></header>
			</Column>
			<Column width="12em">
				<header></header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>